{
    "name": "ADB over TCP/IP",
    "version": "0.1",
    "slug": "adb",
    "description": "ADB over WiFi Interface for Home Assistant",
    "url": "https://github.com/carlymarie/hassio-addons/tree/master/adb",
    "startup": "before",
    "boot": "auto",
    "map": ["config:rw", "ssl"],
    "hassio_api": true,
    "arch": ["aarch64", "amd64", "armv7", "i386"],
    "options": {
        "adb_host": "10.0.0.8",
        "adb_port": 5555
    },
    "schema": {
        "adb_host": "str",
        "adb_port": "int"
    }
}
